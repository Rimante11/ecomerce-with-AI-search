{
  "version": 2,
  "functions": {
    "api/[...endpoint].js": {
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...endpoint].js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}